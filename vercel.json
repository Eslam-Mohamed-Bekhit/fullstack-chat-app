{
    "version": 2,
    "builds": [
        {
            "src": "./server.js",
            "use": "@vercel/node@16.14.0"
        },
        {
            "src": "./public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}